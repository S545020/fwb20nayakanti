extends layout

block content
  h1= title
  p Welcome to #{title}
  - var results = [{color:"Red", price: 10.0, size:"small"},{color:"Pink", price: 20.0, size:"Medium"},{color:"Black", price: 40.0, size:"Large"}]
   div(class="container-fluid")
    div(class="row")
     each form in results
      div(class="col-sm-3") color: #{form.color} - price: #{form.price} - size: #{form.size}